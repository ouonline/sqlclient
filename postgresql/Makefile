ifeq ($(debug), y)
	CXXFLAGS += -g
else
	CXXFLAGS += -O2 -DNDEBUG
endif

AR := ar

TARGET := libpgsql_client_static.a

.PHONY: all clean distclean

all: $(TARGET)

omake_dep_0.pgsql_client.cpp.o: pgsql_client.cpp
	$(CXX) $(CXXFLAGS) -Wall -Werror -Wextra -c $< -o $@

pgsql_client_static_OBJS := omake_dep_0.pgsql_client.cpp.o

