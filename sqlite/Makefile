CXX := g++

ifeq ($(debug), y)
	CXXFLAGS += -g
else
	CXXFLAGS += -O2 -DNDEBUG
endif

AR := ar

TARGET := libsqlite_client_static.a

.PHONY: all clean distclean

all: $(TARGET)

omake_dep_0_INCLUDE := -I../../../..

omake_dep_0.sqlite_client.cpp.o: sqlite_client.cpp
	$(CXX) $(CXXFLAGS) -Wall -Werror -Wextra $(omake_dep_0_INCLUDE) -c $< -o $@

sqlite_client_static_OBJS := omake_dep_0.sqlite_client.cpp.o

libsqlite_client_static.a: $(sqlite_client_static_OBJS)
	$(AR) rc $@ $^

clean:
	rm -f $(TARGET) $(sqlite_client_static_OBJS)

distclean:
	$(MAKE) clean
