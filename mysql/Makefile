ifeq ($(debug), y)
	CXXFLAGS += -g
else
	CXXFLAGS += -O2 -DNDEBUG
endif

AR := ar

TARGET := libmysql_client_static.a

.PHONY: all clean distclean

all: $(TARGET)

omake_dep_0.mysql_client.cpp.o: mysql_client.cpp
	$(CXX) $(CXXFLAGS) -Wall -Werror -Wextra -c $< -o $@

